typedef enum {
{%- for item in obj.items %}
{%- if item.default %}
    {{item.c_define}} = {{item.default}},
{%- else %}
    {{item.c_define}},
{%- endif %}
{%- endfor %}
} {{obj.c_type}};

{%- if obj.export('ToString') == Export.PUBLIC %}
const gchar *{{obj.c_method('ToString')}}({{obj.c_type}} val);
